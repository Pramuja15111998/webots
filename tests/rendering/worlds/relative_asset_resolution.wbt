#VRML_SIM R2022b utf8

EXTERNPROTO "webots://tests/default/protos/TestSuiteEmitter.proto"
EXTERNPROTO "webots://tests/default/protos/TestSuiteSupervisor.proto"
EXTERNPROTO "../protos/ExposedTexture.proto"
EXTERNPROTO "../protos/InternalTexture.proto"
EXTERNPROTO "../protos/depth_2/SolidWithoutParameter_NestedInternalTexture.proto"
EXTERNPROTO "../protos/depth_2/SolidWithoutParameter_NestedDefaultExposedTexture.proto"
EXTERNPROTO "../protos/depth_2/SolidWithoutParameter_NestedOverwrittenExposedTexture.proto"
EXTERNPROTO "../protos/depth_2/SolidWithParameter.proto"

WorldInfo {
}
Viewpoint {
  position -40 0 7
}
Background {
  skyColor [
    0.933333 0.933333 0.92549
  ]
}
# basenode case
Shape {
  appearance PBRAppearance {
    baseColorMap ImageTexture {
      url "../colors/pink_texture.jpg"  # texture relative to the world
    }
  }
  geometry Box {
  }
}
# direct cases
Transform {
  translation 0 -4 0
  children [
    InternalTexture { # not exposed => url is relative to InternalTexture.proto
    }
  ]
}
Transform {
  translation 0 -4 2
  children [
    ExposedTexture { # exposed but default value => relative to ExposedTexture.proto
    }
  ]
}
Transform {
  translation 0 -4 4
  children [
    ExposedTexture {
      exposed_url "../colors/blue_texture.jpg"  # exposed and non-default value => relative to the world
    }
  ]
}
# nested cases - short scope
Transform {
  translation 0 -8 0
  children [
    SolidWithoutParameter_NestedInternalTexture { # not exposed => url relative to InternalTexture.proto
    }
  ]
}
Transform {
  translation 0 -8 2
  children [
    SolidWithoutParameter_NestedDefaultExposedTexture { # exposed but default value => url relative to ExposedTexture.proto
    }
  ]
}
Transform {
  translation 0 -8 4
  children [
    SolidWithoutParameter_NestedOverwrittenExposedTexture { # exposed and overwritten => url relative to SolidWithoutParameter_NestedOverwrittenExposedTexture.proto
    }
  ]
}
# nested cases - long scope
Transform {
  translation 0 -12 0
  children [
    SolidWithParameter { # uninterrupted IS chain up to the world and the world doesn't overwrite it => url relative to SolidWithParameter.proto
    }
  ]
}
